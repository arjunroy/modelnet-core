<!doctype debiandoc public "-//DebianDoc//DTD DebianDoc//EN"
[

    <!-- textual data entities -->
    <!entity titletoc		system "modelnet-doc/titletoc.sgml">
    <!entity ch-intro		system "modelnet-doc/intro.sgml">
    <!entity ch-install		system "modelnet-doc/install.sgml">
    <!entity ch-model		system "modelnet-doc/model.sgml">
    <!entity ch-deploy		system "modelnet-doc/deploy.sgml">
    <!entity ch-execute		system "modelnet-doc/execute.sgml">
    <!entity ch-validate	system "modelnet-doc/validate.sgml">

]
>

<debiandoc>

  <book id="modelnet-doc">

    &titletoc;

    &ch-intro;
    &ch-install;
    &ch-model;
    &ch-deploy;
    &ch-execute;
    &ch-validate;

  </book>

</debiandoc>

